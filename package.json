{
  "name": "proyecto",
  "private": true,
  "workspaces": [
    "."
  ],
  "version": "0.1.0",
  "scripts": {
    "dev": "docker compose up -d postgres && next dev --turbo",
    "devv": "sudo fuser -k 3000/tcp || npm run dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "pkill -9 node || true && rm -rf .next && npm run build && npm run dev",
    "db:up": "docker compose up -d postgres",
    "db:down": "docker compose down",
    "db:psql": "docker exec -it $(docker ps -qf name=postgres) psql -U postgres",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "node scripts/seed.js",
    "presentaciones:auto": "docker compose up -d postgres && node scripts/asignarPresentacionesAutomaticas.mjs",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@cypress/request": "^3.0.9",
    "@prisma/client": "5.9.1",
    "ai": "^6.0.49",
    "axios": "^1.6.7",
    "chart.js": "^4.4.2",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.0.1",
    "cheerio": "^1.0.0-rc.12",
    "convert": "^5.0.0",
    "create-index": "^2.6.0",
    "date-fns": "^3.6.0",
    "dotenv-cli": "^7.3.0",
    "html5-qrcode": "^2.3.8",
    "jose": "^5.2.4",
    "next": "^15.1.6",
    "node-fetch": "^3.3.2",
    "ollama": "^0.6.3",
    "ollama-ai-provider": "^1.2.0",
    "pg": "^8.11.3",
    "puppeteer": "^22.4.0",
    "qs": "^6.14.1",
    "react": "^19.0.0",
    "react-barcode": "^1.6.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.4.6",
    "react-icons": "^5.5.0",
    "server-only": "^0.0.1",
    "sharp": "^0.34.5",
    "sweetalert2": "^11.6.13",
    "sweetalert2-react-content": "^5.0.7"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "autoprefixer": "^10.4.17",
    "cypress": "^15.8.1",
    "eslint": "^8",
    "eslint-config-next": "^15.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "prisma": "5.9.1",
    "tailwindcss": "^3.4.1"
  },
  "overrides": {
    "qs": "^6.14.1"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}